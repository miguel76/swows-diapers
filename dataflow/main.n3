:quantities
 a spinx:LoggedGraph ;
 spinx:input [
  a spinx:DataflowGraph ;
  spinx:input :config ;
  spinx:config [
    a spinx:IncludedGraph ;
    spinx:url <quantities.n3> ;
    spinx:syntax rdfSyntax:N3 ] ] .

:input
a spinx:LoggedGraph ;
 spinx:input swi:InputDataset .


