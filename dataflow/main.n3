@prefix :        <http://www.cattid.uniroma1.it/2012/pampers#> .
@prefix svg:     <http://uispin.org/svg#> .
@prefix uix:     <http://www.swows.org/uix#> .
@prefix tuio:    <http://www.swows.org/tuio#> .
@prefix xml:    <http://www.swows.org/xml#> .
@prefix sp:      <http://spinrdf.org/sp#> .
@prefix spinx:   <http://www.swows.org/spinx#> .
@prefix xsd:     <http://www.w3.org/2001/XMLSchema#> .
@prefix vp:   <http://www.swows.org/viewport#> .
@prefix swi:   <http://www.swows.org/instance#> .
@prefix rdfSyntax:   <http://www.swows.org/syntaxes/RDF/> .
@prefix syntax:   <http://www.swows.org/syntaxes/> .
@prefix query: <http://jena.hpl.hp.com/2003/07/query/> .

swi:OutputDataset
  a spinx:DataflowGraph ;
  spinx:input
    [ a spinx:InlineDataset ;
       spinx:input
        :instantData ;
      spinx:namedInput 
        [ a spinx:NamedGraph ;
          spinx:id :selectedPage ;
          spinx:input :selectedPage ] ;
      spinx:namedInput 
        [ a spinx:NamedGraph ;
          spinx:id :config ;
          spinx:input :config ] ;
      spinx:namedInput 
        [ a spinx:NamedGraph ;
          spinx:id :boxesPositions ;
          spinx:input :boxesPositions ] ;
      spinx:namedInput 
        [ a spinx:NamedGraph ;
          spinx:id :possibleBoxesEnum ;
          spinx:input :possibleBoxesEnum ] ] ;
	 spinx:config
    [ a spinx:IncludedGraph ;
      spinx:url <svg.n3> ;
      spinx:syntax rdfSyntax:N3 ] .

:possibleBoxesEnum
 a spinx:LoggedGraph ;
 spinx:input [
  a spinx:IntegerRangeFromGraph ;
  spinx:input
    [ a spinx:ConstructGraph ;
      spinx:input :config ;
      spinx:config [
        a spinx:IncludedGraph ;
        spinx:url <possibleBoxesEnumInput.sparql> ;
        spinx:syntax query:SPARQL_11 ] ] ].

:instantData
  a spinx:LoggedGraph ;
  spinx:input [
  a spinx:UnionGraph ;
  spinx:input :quantities ;
  spinx:input :colors  ]
 .

:colors
 a spinx:LoggedGraph ;
 spinx:input [
  a spinx:DataflowGraph ;
  spinx:input :quantity ;
  spinx:config [
    a spinx:IncludedGraph ;
    spinx:url <colors.n3> ;
    spinx:syntax rdfSyntax:N3 ] ] .

:boxesPositions
 a spinx:LoggedGraph ;
 spinx:input [
  a spinx:DataflowGraph ;
  spinx:input
    [ a spinx:InlineDataset ;
      spinx:input :quantities ;
      spinx:namedInput 
        [ a spinx:NamedGraph ;
          spinx:id :config ;
          spinx:input :config ] ;
      spinx:namedInput 
        [ a spinx:NamedGraph ;
          spinx:id :possibleBoxesEnum ;
          spinx:input :possibleBoxesEnum ] ;
      spinx:namedInput 
        [ a spinx:NamedGraph ;
          spinx:id :selectedPage ;
          spinx:input :selectedPage ] ;
      spinx:namedInput 
        [ a spinx:NamedGraph ;
          spinx:id :clock ;
          spinx:input :input ] ] ;
  spinx:config [
    a spinx:IncludedGraph ;
    spinx:url <boxesPositions.n3> ;
    spinx:syntax rdfSyntax:N3 ] ] .

:quantity
 a spinx:LoggedGraph ;
 spinx:input [
  a spinx:UnionGraph ;
  spinx:input [
    a spinx:ConstructGraph ;
    spinx:input
      [ a spinx:InlineDataset ;
        spinx:input :quantities ;
        spinx:namedInput 
          [ a spinx:NamedGraph ;
            spinx:id :selectedPage ;
            spinx:input :selectedPage ] ] ;
    spinx:config [
      a spinx:IncludedGraph ;
      spinx:url <selectedQuantity.sparql> ;
      spinx:syntax query:SPARQL_11 ] ] ;
  spinx:input [
    a spinx:ConstructGraph ;
    spinx:input :selectedPage ;
    spinx:config [
      a spinx:IncludedGraph ;
      spinx:url <zeroQuantityIfNotSelected.sparql> ;
      spinx:syntax query:SPARQL_11 ] ] ] .

:config
  a spinx:IncludedGraph ;
  spinx:url <config.n3> ;
  spinx:syntax rdfSyntax:N3 .

:selectedPage
 a spinx:LoggedGraph ;
 spinx:input [
  a spinx:DataflowGraph ;
  spinx:input
    [ a spinx:InlineDataset ;
      spinx:input :input ;
      spinx:namedInput 
        [ a spinx:NamedGraph ;
          spinx:id :config ;
          spinx:input :config ] ] ;
  spinx:config [
    a spinx:IncludedGraph ;
    spinx:url <pageChange.n3> ;
    spinx:syntax rdfSyntax:N3 ] ] .
