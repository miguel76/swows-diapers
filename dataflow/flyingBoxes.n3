swi:OutputDataset
  a spinx:LoggedGraph ;
  spinx:input [
    a spinx:UpdatableGraph ;
    spinx:addGraph [
      a spinx:ConstructGraph ;
      spinx:input
        [ a spinx:InlineDataset ;
          spinx:input :quantityCurr ;
          spinx:namedInput 
            [ a spinx:NamedGraph ;
              spinx:id :flyingBoxes ;
              spinx:input swi:OutputDataset ] ;
          spinx:namedInput 
            [ a spinx:NamedGraph ;
              spinx:id :boxesStartingData ;
              spinx:input :boxesStartingData ] ;
          spinx:namedInput 
            [ a spinx:NamedGraph ;
              spinx:id :clock ;
              spinx:input :clock ] ] ;
      spinx:config [
        a spinx:IncludedGraph ;
        spinx:url <flyingBoxesNew.sparql> ;
        spinx:syntax query:SPARQL_11 ] ] ;
    spinx:deleteGraph [
      a spinx:ConstructGraph ;
      spinx:input
        [ a spinx:InlineDataset ;
          spinx:input :quantityCurr ;
          spinx:namedInput 
            [ a spinx:NamedGraph ;
              spinx:id :flyingBoxes ;
              spinx:input swi:OutputDataset ] ;
          spinx:namedInput 
            [ a spinx:NamedGraph ;
              spinx:id :clock ;
              spinx:input :clock ] ] ;
      spinx:config [
        a spinx:IncludedGraph ;
        spinx:url <flyingBoxesDelete.sparql> ;
        spinx:syntax query:SPARQL_11 ] ] ] .

