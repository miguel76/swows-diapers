@prefix :        <http://www.cattid.uniroma1.it/2012/pampers#> .
@prefix svg:     <http://uispin.org/svg#> .
@prefix uix:     <http://www.swows.org/uix#> .
@prefix tuio:    <http://www.swows.org/tuio#> .
@prefix xml:    <http://www.swows.org/xml#> .
@prefix sp:      <http://spinrdf.org/sp#> .
@prefix spinx:   <http://www.swows.org/spinx#> .
@prefix xsd:     <http://www.w3.org/2001/XMLSchema#> .
@prefix vp:   <http://www.swows.org/viewport#> .
@prefix swi:   <http://www.swows.org/instance#> .
@prefix rdfSyntax:   <http://www.swows.org/syntaxes/RDF/> .
@prefix syntax:   <http://www.swows.org/syntaxes/> .
@prefix query: <http://jena.hpl.hp.com/2003/07/query/> .

swi:OutputDataset
  a spinx:ConstructGraph ;
  spinx:input
    [ a spinx:InlineDataset ;
      spinx:input :allPositions ;
      spinx:namedInput 
        [ a spinx:NamedGraph ;
          spinx:id :selectedPage ;
          spinx:input :selectedPage ] ] ;
  spinx:config [
    a spinx:IncludedGraph ;
    spinx:url <selectedBoxesPositions.sparql> ;
    spinx:syntax query:SPARQL_11 ] .

:allPositions
 a spinx:LoggedGraph ;
 spinx:input [
  a spinx:UnionGraph ;
  spinx:input [
  a spinx:ConstructGraph ;
  spinx:input
    [ a spinx:InlineDataset ;
      spinx:input swi:InputDataset ;
      spinx:namedInput 
        [ a spinx:NamedGraph ;
          spinx:id :boxesStartingData ;
          spinx:input :boxesStartingData ] ] ;
  spinx:config [
    a spinx:IncludedGraph ;
    spinx:url <boxesPositions.sparql> ;
    spinx:syntax query:SPARQL_11 ] ] ;
  spinx:input [
  a spinx:ConstructGraph ;
  spinx:input
    [ a spinx:InlineDataset ;
      spinx:input :flyingBoxes ;
      spinx:namedInput 
        [ a spinx:NamedGraph ;
          spinx:id :config ;
          spinx:input :config ] ;
      spinx:namedInput 
        [ a spinx:NamedGraph ;
          spinx:id :boxesStartingData ;
          spinx:input :boxesStartingData ] ;
      spinx:namedInput 
        [ a spinx:NamedGraph ;
          spinx:id :clock ;
          spinx:input :clock ] ] ;
  spinx:config [
    a spinx:IncludedGraph ;
    spinx:url <flyingBoxesPositions.sparql> ;
#    spinx:url <emptyConstruct.sparql> ;
    spinx:syntax query:SPARQL_11 ] ] ] .

:boxesStartingData
 a spinx:LoggedGraph ;
 spinx:input [
  a spinx:ConstructGraph ;
  spinx:input
    [ a spinx:InlineDataset ;
      spinx:input
        :possibleBoxesEnum ;
      spinx:namedInput 
        [ a spinx:NamedGraph ;
          spinx:id :config ;
          spinx:input :config ] ] ;
  spinx:config [
    a spinx:IncludedGraph ;
    spinx:url <boxesStartingData.sparql> ;
    spinx:syntax query:SPARQL_11 ] ] .

:flyingBoxes
 a spinx:LoggedGraph ;
 spinx:input [
  a spinx:DataflowGraph ;
  spinx:input
    [ a spinx:InlineDataset ;
      spinx:input swi:InputDataset ;
      spinx:namedInput 
        [ a spinx:NamedGraph ;
          spinx:id :boxesStartingData ;
          spinx:input :boxesStartingData ] ;
      spinx:namedInput 
        [ a spinx:NamedGraph ;
          spinx:id :clock ;
          spinx:input :clock ] ] ;
  spinx:config [
    a spinx:IncludedGraph ;
    spinx:url <flyingBoxes.n3> ;
    spinx:syntax rdfSyntax:N3 ] ] .

#:flyingBoxes
#  a spinx:IncludedGraph ;
#  spinx:url <flyingBoxes.n3> ;
#  spinx:syntax rdfSyntax:N3 .

:config
  a spinx:SelectGraph ;
  spinx:input swi:InputDataset ;
  spinx:id :config .

:clock
# a spinx:LoggedGraph ;
# spinx:input [
  a spinx:SelectGraph ;
  spinx:input swi:InputDataset ;
  spinx:id :clock .

:possibleBoxesEnum
  a spinx:SelectGraph ;
  spinx:input swi:InputDataset ;
  spinx:id :possibleBoxesEnum .

:selectedPage
  a spinx:SelectGraph ;
  spinx:input swi:InputDataset ;
  spinx:id :selectedPage .


