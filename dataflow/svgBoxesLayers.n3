swi:OutputDataset
  a spinx:UnionGraph ;
  spinx:input
    :svgBoxesLayersEach, :svgBoxesLayersFirst, :svgBoxesLayersNext .

:svgBoxesLayersEach
  a spinx:ConstructGraph ;
  spinx:input
    swi:InputDataset ;
  spinx:config
    [ a spinx:IncludedGraph ;
      spinx:url <svgBoxesLayersEach.sparql> ;
      spinx:syntax query:SPARQL_11 ] .

:svgBoxesLayersFirst
